  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seegno/eslint-config-seegno/issues"
  },
  "homepage": "https://github.com/seegno/eslint-config-seegno#readme",
  "devDependencies": {
    "babel-eslint": "^6.0.0",
    "eslint": "^2.5.1",
    "eslint-plugin-babel": "^3.1.0",
    "mocha": "^2.4.5",
    "should": "^8.3.0"
  },
  "engines": {
    "node": ">=4"
  }
}
